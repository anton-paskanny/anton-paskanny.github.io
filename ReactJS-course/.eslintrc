{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": [1, 120, 2, { "ignoreComments": true } ],
        "react/prop-types": 0
    },
    "globals": {
        "document": true,
        "localStorage": true,
        "window": true,
        "fetch": true
    },
    "extends": [ "airbnb-base", "eslint:recommended", "plugin:react/recommended" ]
}