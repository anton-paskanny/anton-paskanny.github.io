{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
  },
  "env": {
        "es6": true,
        "browser": true,
        "node": true
  },
  "extends": "airbnb-base",
  "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "max-len": ["error", 120]
  }
}
